.stxq{display: block;width:100%;height:auto;}
.stxq .tops{display: block;width:100%;height:100px;border-bottom:3px solid #eee}
.stxq .tops .l{display: block;width:26%;height:100%;float:left}
.stxq .tops .l .l_l{display: block;width:80px;height:80px;margin:8% auto;border-radius:2px}
.stxq .tops .l .l_l image{display: block;width:100%;height:100%}
.stxq .tops .r{display: block;float:right;width:72%;height:100%;}
.stxq .tops .r .t{display: block;width:100%;height:25px;line-height:25px;margin-top:9px;font-size: 14px;font-weight: bolder}
.stxq .tops .r .c{display: block;width:100%;height:20px;line-height:20px;margin-top:3px;font-size: 12px;color:#999}
.stxq .tops .r .b{display: block;width:100%;height:20px;line-height:20px;margin-top:4px;font-size: 12px;color:#999;overflow: hidden}
.center{display: block;width:95%;height:auto;margin:0 auto;margin-top:3%;margin-bottom:8%}
.center li{display: block;width:100%;height:auto;background:#fff;border-radius:5px;margin-top:5px;margin-bottom:3%}
.center li .top{display: block;width:100%;height:40px;line-height: 40px;}
.center li .top .l{display: block;float:left;width:70%;height:100%;}
.center li .top .l image{width:33px;height:33px;border-radius:50%;display: block;margin-left:2%;margin-top:1px}
.center li .top text{display:block;width:79%;height:100%;font-size:14px;color:#999;float:right;margin-top:-38px;line-height:48px}
.center li .top .r{display: block;float:right;width:26%;height:100%;text-align: right;line-height:40px;font-size:14px;padding:0 8px 0 0;color:#999}
.center li .title{display: block;width:96%;height:30px;line-height: 30px;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;font-size:13px;padding:0 4px}
.center li .content{display: block;width:96%;height:auto;line-height:18px;overflow: hidden;font-size:13px;padding:0 4px;word-break: break-all;word-wrap: break-word;color:#999}
.center li .image{display: block;width:96%;height:auto;line-height:18px;overflow: hidden;font-size:13px;padding:0 4px;}
.center li .image .one_video{display: block;width:100%;height:120px;margin-top:5px;margin-bottom:10px;margin:0 auto;position: relative;border:1px solid red}
.center li .image  .uploadimg{display: block;width:100%;height:140px;margin-top:5px;margin:0 auto;position: relative;margin-bottom:20px;}
.center li .image  .uploadimg video{display: block;width:100%;height:100%;}
.center li .image .one_video .img{display: block;width:100%;height:100%;margin:0 auto}
.center li .image .one_pic{display: block;width:100%;height:140px;margin-top:5px;margin-bottom:5px}
.center li .image .one_pic image{display: block;width:100%;height:100%}
.center li .image .two_pic{display:inline-block;width:30%;height:100px;margin-top:5px;margin-bottom:5px;margin-left:5px;}
.center li .image .two_pic image{display: block;width:100%;height:100%}
.center li .image .three_pic{display:inline-block;width:31%;height:100px;margin-top:5px;margin-bottom:5px;margin-left:4.8px;}
.center li .image .three_pic image{display: block;width:100%;height:100%}
.center li .bott{display: block;width:100%;height:35px;line-height: 38px;margin-top:-3%}
.center li .bott label{display:inline-block;width:31.5%;margin-left:3px;line-height:35px;height:30px;text-align: center;font-size:12px;color:#999;}
.center li .bott label image{display:block;width:18px;height:18px;margin-left:15%;margin-top:6%;}
.center li .bott label i{display:block;height:28px;width:60%;margin-left:5%;margin-top:-27px;float:right;text-align: left}
.center li .d{display: block;width:100%;height:10px;line-height:10px}
.center .ajax_onload{display:block;width:60%;height:28px;line-height: 28px;text-align: center;margin-top:8px;font-size:14px;color:#999;margin:0 auto;}
.float-icon {position:fixed;
z-index:20;
right:50rpx;
bottom:140rpx;
}
.float-icon .float-icon-btn {
display:block;
padding:0;
margin:0;
border:none;
background:none;
}
.float-icon .float-icon-btn image {
width:86rpx;
height:86rpx;
display:block;
}
.r_r{display: block;float: right;width:20%;height:100%;margin-top:-20%;
}
.r_r .join{display: block;width:90%;height:30px;margin:20% auto;background:#F7D86F;color:#fff;text-align: center;line-height: 30px;font-size:13px;margin-right:7px;border-radius:8px;}

