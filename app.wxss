/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.we7-bottom-placeholder {
    width: 100%;
    height: 91rpx;
    position: relative;
    bottom: 0;
    z-index: -1;
}

.we7-bottom {
    display: flex;
    overflow: hidden;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    padding: 12rpx 0 12rpx 0;
    border-top: 1px solid #999;
    background-color: #fff;
    z-index: 99999;
}

.we7-bottom.active {
    background-color: #fff;
}

.we7-bottom.active navigatorurl {
    background-color: #fff;
}

.we7-bottom .we7-bottom-item {
    flex: 1;
    padding: 5rpx 10rpx 0rpx 10rpx;
    text-align: center;
}

.we7-bottom .item-img {
    width: 48rpx;
    height: 48rpx;
    display: block;
    overflow: hidden;
    margin: 0 auto;
}

.we7-bottom .item-text {
    display: block;
    font-size: 12px;
    height: 15px;
    line-height: 15px;
    overflow: hidden;
}

